{
  "name": "react-rsbuild-electron",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "npm run dev:render",
    "dev:render": "cross-env PORT=8088 rsbuild dev --config ./builder/render/rsbuild.dev.ts",
    "build": "rsbuild build --config ./builder/render/rsbuild.build.ts",
    "preview": "rsbuild preview"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@rsbuild/core": "^0.7.0",
    "@rsbuild/plugin-react": "^0.7.0",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "cross-env": "^7.0.3",
    "typescript": "^5.4.2"
  }
}
