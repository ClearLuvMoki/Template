{
  "name": "12",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "npm run dev:render",
    "dev:render": "cross-env PORT=8088 rsbuild dev --config ./builder/render/rsbuild.dev.ts",
    "dev:main": "cross-env PORT=8088  nodemon --watch ./src/main/index.ts --exec npm run rebuild:main",
    "rebuild:main": "npm run build:main && electron ./release/dist/main/index.js",
    "build:main": "rsbuild build --config ./builder/main/rsbuild.build.ts",
    "build": "rsbuild build",
    "preview": "rsbuild preview"
  },
  "dependencies": {
    "electron": "^30.0.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@rsbuild/core": "^0.7.0",
    "@rsbuild/plugin-react": "^0.7.0",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.1",
    "typescript": "^5.4.2"
  }
}
